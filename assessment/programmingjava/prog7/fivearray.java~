/* *****************************************AUTHOR Paul CurzonThis program demonstrates  - arrays loops.   Fill an array using a for loop then print it out   ******************************************** */import javax.swing.*; // import the swing library for I/Oclass fivearray{    public static void main (String[] param)    {                fiveArray();        System.exit(0);            } // END main   /* ***************************************************       Define some commands of our own to use above   *************************************************** */            /* ***************************************************       Set up an array contianing the number 5  everywhere then print it    */        public static void fiveArray()    {       // Declare variables       //              int[] fives = new int[12];       //first fill an array with the number 5 everywhere       for(int i = 0; i<fives.length; i++)       {           fives[i] = 5;       }                   //now print the contents to show what we did       for(int i = 0; i<fives.length; i++)       {           System.out.println(i + ":" + fives[i]);       }          } // END fiveArray    } // END class fivearray 